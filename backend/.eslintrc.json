{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "plugins": ["prettier"],
    "rules": {
        "no-console": 1
    },
    "globals": {
        "axios": true,
        "PropTypes": true
    }
}
